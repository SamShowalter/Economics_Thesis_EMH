#work for any categorical variable
makePieChart = function(data)
{
data_table = table(data)
names_labs = names(data_table)
percent = round(data_table / sum(data_table) * 100)
names_labs = paste(names_labs,paste("\n(",percent,"%)",sep = ""))
opar = par(no.readonly = TRUE)
par(col="Orange")
pie(data_table, labels = names_labs, border = "black", main = "Automatically Created Pie Chart for Data")
par(opar)
legend("bottomright",legend = names(data_table), fill = c("White","Blue","Pink"))
}
makePieChart(c(rep("Yellow",34),rep("Blue",23),rep("Purple",65)))
